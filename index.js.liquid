    <!-- Carousel -->
    <script id="sjs-carousel" defer type="module">
      import { Carousel, LazySource } from './packages/es/index.js';

      const hwCarousel = new Carousel();

      hwCarousel.Mount('.hardware');

      const swCarousel = new Carousel();

            hwCarousel.Mount('.hardware');
            hwCarousel.SlideInterval(4000);
      swCarousel.Mount('.software');

      const lazySrcs = new LazySource();

            swCarousel.Mount('.software');
            swCarousel.SlideInterval(7000);
      lazySrcs.Mount('[data-src]');

      document.getElementById("carousel-action").onmouseover = function StopCarousel() {
        hwCarousel.StopInterval();
        swCarousel.StopInterval();
    }

    document.getElementById("carousel-action").onmouseleave = function StartCarousel() {
        hwCarousel.SlideInterval(4000);
        swCarousel.SlideInterval(7000);
    }
    </script>

