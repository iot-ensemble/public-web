title: Test Title
layout: main.liquid
---
 
{% comment %} <div id="product-view">
  <img alt="" src="{{ image }}" height="959" width="640" />
 
  <form class="body">
    <h1>{{ name }}</h1>
 
    <p class="price">{{ price | money }}</p>
 
    <h2>Product Description</h2>
 
    <p>{{ description }}</p>
 
    <div>
      <jamcart-add
        include-form-data
        image="{{ image }}"
        name="{{ title }}"
        price="{{ price }}"
        url="{{ page.url }}"></jamcart-add>
    </div>
  </form>
</div> {% endcomment %}